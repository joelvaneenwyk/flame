{
  "name": "flame",
  "version": "0.1.0",
  "description": "Self-hosted start page",
  "main": "index.js",
  "private": true,
  "workspaces": [
    "client"
  ],
  "scripts": {
    "start": "yarn node server.js",
    "init-server": "echo Installing server dependencies",
    "init-client": "echo Installing client dependencies",
    "dir-init": "npx mkdirp data public && touch public/flame.css public/customQueries.json",
    "dev-init": "yarn dir-init && yarn init-server && yarn init-client",
    "dev-server": "nodemon server.js -e js",
    "dev-client": "yarn start --prefix client",
    "dev": "concurrently \"yarn dev-server\" \"yarn dev-client\"",
    "skaffold": "concurrently \"yarn init-client\" \"yarn dev-server\"",
    "build": "yarn workspaces foreach -A run build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@kubernetes/client-node": "^0.20.0",
    "@testing-library/dom": "^10.0.0",
    "@types/express": "^4.17.21",
    "axios": "^1.6.8",
    "client": "workspace:*",
    "concurrently": "^8.2.2",
    "docker-secret": "^2.0.6",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.3",
    "node-schedule": "^2.1.1",
    "react": "^18.2.0",
    "react-router": "^6.23.0",
    "sequelize": "^6.37.3",
    "sqlite3": "^5.1.7",
    "umzug": "^3.8.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.0"
  },
  "packageManager": "npm@10.5.2+sha256.df0a1f7691654b94786013e5d5b80a873a7ffbae9c82c3bec8b5db76bc3b7dfd"
}
